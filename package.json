{
  "name": "hanbok",
  "version": "0.0.0",
  "description": "Opinionated platform for developing and deploying web apps in AWS",
  "license": "MIT",
  "author": "Nathan Abramson",
  "private": false,
  "main": "dist/lib/lib.es.js",
  "module": "dist/lib/lib.es.js",
  "types": "dist/lib/lib.d.ts",
  "sideEffects": false,
  "files": [
    "dist"
  ],
  "bin": {
    "hanbok": "bin/hanbok",
    "hb": "bin/hb"
  },
  "oclif": {
    "bin": "hanbok",
    "commands": {
      "strategy": "explicit",
      "target": "./dist/cli/cli.es.js",
      "identifier": "COMMANDS"
    },
    "plugins": [
      "@oclif/plugin-help",
      "@oclif/plugin-not-found",
      "@oclif/plugin-autocomplete"
    ],
    "topicSeparator": " "
  },
  "type": "module",
  "scripts": {
    "build": "npm run esbuild; npm run tsc; npm run rollup",
    "dev": "npx concurrently npm:esbuild-watch npm:tsc-watch npm:rollup-watch",
    "tsc": "npx tsc -b",
    "tsc-watch": "npx tsc -b -w",
    "esbuild": "node ./esbuild.config.js",
    "esbuild-watch": "node ./esbuild.config.js --watch",
    "rollup": "npx rollup -c rollup.config.js",
    "rollup-watch": "npx rollup -w -c rollup.config.js",
    "prettier": "npx prettier --write **/*.{ts,tsx,css,scss,html,json,js}"
  },
  "dependencies": {
    "chokidar": "^4.0.3",
    "oclif": "^4.17.46",
    "pkg-dir": "^8.0.0",
    "prisma": "^6.6.0",
    "rollup": "^4.40.0",
    "vite": "^6.3.3"
  },
  "peerDependenciesMeta": {},
  "devDependencies": {
    "@oclif/core": "^4.2.10",
    "@oclif/plugin-autocomplete": "^3.2.27",
    "@oclif/plugin-help": "^6.2.27",
    "@oclif/plugin-not-found": "^3.2.49",
    "@types/node": "^22.14.1",
    "concurrently": "^9.1.2",
    "esbuild": "^0.25.2",
    "prettier": "^3.5.3",
    "rollup-plugin-dts": "^6.2.1",
    "typescript": "~5.8.3"
  }
}
